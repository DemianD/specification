@prefix tree: <https://w3id.org/tree#>.
@prefix hydra: <http://www.w3.org/ns/hydra/core#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <http://shema.org/>.
@prefix shacl: <http://www.w3.org/ns/shacl#>.
@prefix lc: <http://semweb.mmlab.be/ns/linkedconnections#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

<https://graph.irail.be/sncb/connections> a hydra:Collection ;
                                          rdfs:label "All arrivals and departures of Trains in Belgium"@en ;
                                          hydra:manages [ hydra:property <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ; hydra:object lc:Connection  ];
                                          hydra:view <parent.ttl> .

<parent.ttl> a tree:Node, hydra:PartialCollectionView;
             tree:value [
                        a schema:DateTime ;
                        schema:startDate "2019-07-23T06:13:00.000Z"^^xsd:dateTime ;
                        schema:endDate "2019-07-23T06:30:00.000Z" ^^xsd:endTime 
             ] ;
             shacl:path lc:departureTime ; 
             hydra:next <child.ttl> ;
             tree:hasChildRelation [
                                   a tree:GreaterThanRelation ;
                                   tree:child <child.ttl> ;
                                   ].  

<child.ttl> a tree:Node ;
            tree:value [
                       a schema:DateTime ;
                       schema:startDate "2019-07-23T06:30:00.000Z" ^^xsd:endTime
                       ] .

