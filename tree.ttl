tree: a foaf:Document ;
      foaf:primaryTopic tree:Ontology;
      cc:license <http://creativecommons.org/licenses/by/4.0/>;
      dct:creator <https://pietercolpaert.be/#me>.

<https://pietercolpaert.be/#me> foaf:name "Pieter Colpaert"; foaf:mbox "pieter.colpaert@ugent.be".

tree:Ontology a owl:Ontology ;
              rdfs:label "Tree Ontology"@en;
              rdfs:comment "Ontology for describing tree-like relations between hypermedia documents.".

######## Classes

tree:Node a rdfs:Class ;
          rdfs:label "Node"@en;
          rdfs:comment "A tree:Node is a node that may contain links to other dereferenceable resources, may contain the actual data (as part of a named graph, or using the predicate hydra:member)."@en.

######## Properties

tree:hasRootNode a rdf:Property ;
                 rdfs:label "Has root node"@en;
                 rdfs:comment ""@en;
                 rdfs:domain ;
                 rdfs:range hydra:Resource, foaf:Document.

